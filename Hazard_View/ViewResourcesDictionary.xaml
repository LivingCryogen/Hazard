            </Grid>
            <TextBlock Grid.Row="3" Text="{Binding Territory, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=UserControl}}" FontWeight="ExtraBlack" TextAlignment="Center" VerticalAlignment="Center" TextWrapping="WrapWithOverflow" Foreground="Black" />
        </Grid>
    </DataTemplate>

