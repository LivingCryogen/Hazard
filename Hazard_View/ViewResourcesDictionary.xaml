            <Image Height="50" Stretch="Uniform" Source="{StaticResource Cavalry}" />
            <Image Width="55" Grid.Row="2" Stretch="Uniform" Source="{StaticResource Artillery}" />
        </StackPanel>
    </DataTemplate>

