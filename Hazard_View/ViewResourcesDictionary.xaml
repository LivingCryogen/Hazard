        </Style.Setters>
        <Style.Triggers>
            <DataTrigger Value="{x:Static local:Face.Troop}">
                <DataTrigger.Binding>
                    <MultiBinding Converter="{StaticResource CardFaceConverter}">
                        <Binding Path="PlayerTurn" RelativeSource="{RelativeSource Mode=TemplatedParent}"/>
                        <Binding Path="Owner" RelativeSource="{RelativeSource Mode=TemplatedParent}"/>
                        <Binding Path="CardFace" RelativeSource="{RelativeSource Mode=TemplatedParent}"/>
                    </MultiBinding>
                </DataTrigger.Binding>
                <DataTrigger.Setters>
                    <Setter Property="ContentTemplate" Value="{StaticResource TroopCardControlTerritoryTemplate}" />
                </DataTrigger.Setters>
            </DataTrigger>
            <DataTrigger Value="{x:Static local:Face.Wild}">
                <DataTrigger.Binding>
                    <MultiBinding Converter="{StaticResource CardFaceConverter}">
                        <Binding Path="PlayerTurn" RelativeSource="{RelativeSource Mode=TemplatedParent}" />
                        <Binding Path="Owner" RelativeSource="{RelativeSource Mode=TemplatedParent}" />
                        <Binding Path="CardFace" RelativeSource="{RelativeSource Mode=TemplatedParent}"/>
                    </MultiBinding>
                </DataTrigger.Binding>
                <DataTrigger.Setters>
                    <Setter Property="ContentTemplate" Value="{StaticResource TroopCardControlWildTemplate}" />
                </DataTrigger.Setters>
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <ControlTemplate x:Key="CardControlTemplate" TargetType="UserControl" >
        <Grid Background="FloralWhite" Width="121.53" Height="168">
            <Border  BorderBrush="Black" BorderThickness="1" CornerRadius="1" Margin="5">
                <ContentPresenter Style="{StaticResource CardControlContentPresenterStyle}" Content="{TemplateBinding Content}"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="CardControlTemplateTest" TargetType="UserControl" >
